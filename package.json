{
  "name": "supertrend",
  "version": "1.0.2",
  "author": "maxgfr",
  "license": "MIT",
  "description": "Implementation of supertrend indicator",
  "main": "./build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxgfr/supertrend.git"
  },
  "bugs": {
    "url": "https://github.com/maxgfr/supertrend/issues"
  },
  "homepage": "https://github.com/maxgfr/supertrend#readme",
  "files": [
    "build"
  ],
  "keywords": [
    "supertrend",
    "technicalindicators",
    "technical",
    "indicators",
    "trading",
    "finance",
    "cryptocurrency"
  ],
  "scripts": {
    "start": "node build/index.js",
    "dev": "nodemon",
    "develop": "node -r @swc-node/register ./src/index.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "clean": "rimraf build",
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -w -p tsconfig.build.json",
    "build:swc": "swc ./src -d build",
    "build:swc:watch": "swc ./src -d build -w",
    "lint": "eslint ./src --ext .ts",
    "prettier": "prettier --write './src/**/*.{ts,js,json}'",
    "release": "semantic-release"
  },
  "dependencies": {
    "technicalindicators": "^3.1.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.0",
    "@semantic-release/npm": "^13.0.0",
    "@semantic-release/release-notes-generator": "^14.0.0",
    "@swc-node/register": "1.11.1",
    "@swc/cli": "0.7.9",
    "@swc/core": "1.15.3",
    "@swc/jest": "0.2.39",
    "@types/jest": "30.0.0",
    "@types/node": "24.10.4",
    "@typescript-eslint/eslint-plugin": "8.49.0",
    "@typescript-eslint/parser": "8.49.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jest": "29.5.0",
    "jest": "30.2.0",
    "nodemon": "3.1.11",
    "prettier": "3.7.4",
    "rimraf": "6.1.2",
    "semantic-release": "25.0.2",
    "typescript": "5.9.3"
  }
}
